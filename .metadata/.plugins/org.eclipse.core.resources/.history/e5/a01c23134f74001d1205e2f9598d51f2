package com.gregg.carAndDriver;

import com.gregg.carAndDriver.models.Driver;

public class CarAndDriverApplication {

	public static void main(String[] args) {
		
		Driver Gregg = new Driver ();
		Gregg.drive();
		Gregg.drive();
		Gregg.drive();
		Gregg.drive();
		Gregg.useBoosters();
		Gregg.drive();
		Gregg.useBoosters();
		Gregg.refuel();
		Gregg.refuel();
		Gregg.refuel();
	}

}
